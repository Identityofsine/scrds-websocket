{
  "name": "@shared/networking",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/@shared/networking/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/@shared/networking",
        "main": "packages/@shared/networking/src/index.ts",
        "tsConfig": "packages/@shared/networking/tsconfig.lib.json"
      }
    },
    "typecheck": {
      "executor": "@nx/js:tsc",
      "options": {
        "tsConfig": "packages/@shared/networking/tsconfig.lib.json",
        "noEmit": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/@shared/networking/jest.config.ts"
      }
    }
  },
  "tags": []
}
